{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src/web/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "src/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/web/$1"
    }
  ],
  "functions": {
    "src/main.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "PYTHONPATH": "src"
  }
}
