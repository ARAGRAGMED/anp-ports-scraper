{
  "version": 2,
  "name": "anp-ports-vessel-scraper",
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.py"
    }
  ],
  "env": {
    "VERCEL_ENV": "production"
  },
  "functions": {
    "src/main.py": {
      "maxDuration": 30
    }
  }
}
